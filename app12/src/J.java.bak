class I
{
	public static void main(String[] args) 
	{
		System.out.println("main begin");
		int i=1;
		while(i<=5)
		{
		System.out.println("loop body begin"+i);
		for(int j=50;j<=55;j++){
		System.out.println("loop body begin "+i+","+j);
